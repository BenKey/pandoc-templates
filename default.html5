<!DOCTYPE html>
<html $if(lang)$lang="$lang$"$endif$$if(dir)$dir="$dir$"$endif$>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
$for(author-meta)$
  <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$">
$endif$
$if(description-meta)$
  <meta name="description" content="$description-meta$">
$endif$
  <meta name="generator" content="pandoc">
$if(keywords)$
  <meta name="keywords" content="$for(keywords)$$keywords$$sep$, $endfor$">
$endif$
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">
  <title>$if(title-prefix)$$title-prefix$ – $endif$$pagetitle$$if(title-suffix)$ – $title-suffix$$endif$</title>
  <style>
    #skip-to-main-content a {
      clip: rect(1px, 1px, 1px, 1px);
      height: 1px;
      left: -10000px;
      overflow: hidden;
      position: absolute;
      top: auto;
      width: 1px;
    }
    #skip-to-main-content a:active,
    #skip-to-main-content a:focus {
      clip: auto;
      height: auto;
      outline-style: solid !important;
      outline-width: thick !important;
      position: static;
      width: auto;
    }
    $styles.html()$
  </style>
$for(css)$
  <link rel="stylesheet" href="$css$">
$endfor$
$if(math)$
  $math$
$endif$
$for(js)$
  <script src="$js$"></script>
$endfor$
  <!--[if lt IE 9]>
    <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv-printshiv.min.js"></script>
  <![endif]-->
$for(header-includes)$
  $header-includes$
$endfor$
</head>
<body>
<div id="skip-to-main-content" class="hidden-print"><a href="#main-content">Skip to Main Content</a></div>
$for(include-before)$
  $include-before$
$endfor$
$if(website-title)$
<header id="title-block-header">
<div class="website-title">$website-title$</div>
$for(author)$
<p class="author">$author$</p>
$endfor$
$if(date)$
<p class="date">$date$</p>
$endif$
</header>
$else$
$if(title)$
<header id="title-block-header">
<h1 class="title">$title$</h1>
$if(subtitle)$
<p class="subtitle">$subtitle$</p>
$endif$
$for(author)$
<p class="author">$author$</p>
$endfor$
$if(date)$
<p class="date">$date$</p>
$endif$
</header>
$endif$
$endif$
$if(toc)$
<nav id="$idprefix$TOC" role="doc-toc" tabindex="-1">
$if(toc-title)$
<h2 id="$idprefix$toc-title">$toc-title$</h2>
$endif$
$table-of-contents$
</nav>
$endif$
$if(customize-nav-bar)$
<nav class="hidden-print">
<ul class="horizontal-nav-bar">
$for(nav-bar-links)$
$nav-bar-links$
$endfor$
</ul>
</nav>
$endif$
<main>
$if(website-title)$
<h1 class="title" id="main-content" tabindex="-1">$title$</h1>
$if(subtitle)$
<p class="subtitle">$subtitle$</p>
$endif$
$else$
<section id="main-content">
$endif$
$body$
$if(website-title)$
$else$
</section>
$endif$
</main>
$for(include-after)$
$include-after$
$endfor$
</body>
</html>
